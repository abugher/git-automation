cd "${main_single_repo}"

touch thing1
output="$(g automatic 'No comment.')"
version="$(echo "${output}" | tail -n 1)"
test "${version}" == '0.0.0'

touch thing2
output="$(g automatic 'No comment.')"
version="$(echo "${output}" | tail -n 1)"
test "${version}" == '0.0.1'

touch thing3
output="$(g patch 'No comment.')"
version="$(echo "${output}" | tail -n 1)"
test "${version}" == '0.0.2'
